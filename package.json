{
    "$schema": "https://json.schemastore.org/package.json",
    "private": true,
    "type": "module",
    "workspaces": [
        "e2e",
        "resources/ts/Components/ui"
    ],
    "packageManager": "yarn@1.22.22",
    "scripts": {
        "commitlint": "commitlint --edit",
        "build": "vite build",
        "dev": "vite",
        "test": "vitest",
        "test:unit": "vitest",
        "test:unit:watch": "vitest --watch",
        "test:unit:coverage": "vitest --coverage",
        "test:unit:ui": "vitest --ui",
        "test:watch": "vitest --watch",
        "test:coverage": "vitest --coverage",
        "test:e2e": "yarn workspace @examena/e2e-tests test",
        "test:e2e:ui": "yarn workspace @examena/e2e-tests test:ui",
        "test:e2e:debug": "yarn workspace @examena/e2e-tests test:debug",
        "test:e2e:headed": "yarn workspace @examena/e2e-tests test:headed",
        "test:e2e:chrome": "yarn workspace @examena/e2e-tests test:chrome",
        "test:e2e:report": "yarn workspace @examena/e2e-tests test:report",
        "test:e2e:setup": "yarn workspace @examena/e2e-tests test:setup",
        "test:e2e:admin": "yarn workspace @examena/e2e-tests test:admin",
        "test:e2e:teacher": "yarn workspace @examena/e2e-tests test:teacher",
        "test:e2e:student": "yarn workspace @examena/e2e-tests test:student",
        "test:all": "yarn test:unit && yarn test:e2e"
    },
    "devDependencies": {
        "@commitlint/cli": "^19.7.0",
        "@commitlint/config-conventional": "^19.7.0",
        "@tailwindcss/vite": "^4.1.13",
        "@testing-library/dom": "^10.4.1",
        "@testing-library/jest-dom": "^6.9.1",
        "@testing-library/react": "^16.3.0",
        "@testing-library/user-event": "^14.6.1",
        "@types/node": "^25.0.3",
        "@vitejs/plugin-react": "^5.0.3",
        "@vitest/coverage-v8": "^4.0.16",
        "@vitest/ui": "^4.0.16",
        "axios": "^1.11.0",
        "concurrently": "^9.0.1",
        "jsdom": "^27.4.0",
        "laravel-vite-plugin": "^2.0.0",
        "ts-node": "^10.9.2",
        "vite": "^7.0.4",
        "vitest": "^4.0.16"
    },
    "dependencies": {
        "@dnd-kit/core": "^6.3.1",
        "@dnd-kit/sortable": "^10.0.0",
        "@dnd-kit/utilities": "^3.2.2",
        "@heroicons/react": "^2.2.0",
        "@inertiajs/react": "^2.1.6",
        "@types/react": "^19.1.13",
        "@types/react-dom": "^19.1.9",
        "easymde": "^2.20.0",
        "immer": "^11.1.0",
        "katex": "^0.16.23",
        "prismjs": "^1.30.0",
        "react": "^19.1.1",
        "react-dom": "^19.1.1",
        "react-markdown": "^10.1.0",
        "rehype-katex": "^7.0.1",
        "rehype-prism-plus": "^2.0.1",
        "remark-gfm": "^4.0.1",
        "remark-math": "^6.0.0",
        "typescript": "^5.9.2",
        "ziggy-js": "^2.6.0",
        "zod": "^4.1.12",
        "zustand": "^5.0.8"
    }
}
